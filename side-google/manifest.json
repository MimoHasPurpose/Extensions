{
    "name": "side-google",
    "description": "a extension to open google in sidle panel.",
    "version": "1.0",
    "manifest_version": 3,
    
    "background": {
      "service_worker": "service-worker.js"
    },
    "action": {
      "default_title": "side-google"
    },
    "side_panel": {
      "default_path": "main.html"
    },
    "permissions": ["sidePanel"]
  }
